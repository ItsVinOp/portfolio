---
import AboutSVG from "../svgs/sidebar/AboutSVG.astro"
import CareerSVG from "../svgs/sidebar/CareerSVG.astro"
import ProjectSVG from "../svgs/sidebar/ProjectSVG.astro"
import SkillsSVG from "../svgs/sidebar/SkillsSVG.astro"
import ServicesSVG from "../svgs/sidebar/ServicesSVG.astro"
import ContactSVG from "../svgs/sidebar/ContactSVG.astro"
import DownloadSVG from "../svgs/DownloadSVG.astro"

const { as } = Astro.props 
---

{ as==='download'           && <DownloadSVG /> }

<!-- sidebar --------------------------------------------->
{ as==='about'          && <AboutSVG /> }
{ as==='career'         && <CareerSVG /> }
{ as==='project'        && <ProjectSVG /> }
{ as==='skills'         && <SkillsSVG /> }
{ as==='services'       && <ServicesSVG /> }
{ as==='contact'        && <ContactSVG /> }